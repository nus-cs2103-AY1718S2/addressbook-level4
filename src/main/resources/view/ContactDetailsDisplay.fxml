<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="162.0" percentWidth="30.0" />
      <ColumnConstraints hgrow="SOMETIMES" percentWidth="70.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints percentHeight="25.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" percentHeight="75.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <ImageView fx:id="imageView" fitHeight="146.0" fitWidth="155.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
         <GridPane.margin>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </GridPane.margin>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <Label fx:id="name" styleClass="details-name-huge-label" wrapText="true" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
         <GridPane.margin>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </GridPane.margin></Label>

                        <ListView fx:id="keys" minWidth="162.0" prefWidth="162.0" GridPane.rowIndex="1" />
      <ListView fx:id="values" prefWidth="212.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
</GridPane>
